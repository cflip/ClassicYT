                <img alt="" title="Facebook" src="/pixel.gif" class="icon-watch-comment-share icon-watch-comment-share-facebook" action="yt.window.popup(&#39;http:\/\/www.facebook.com\/sharer.php?u=http%3A%2F%2Fwww.youtube.com%2Fcomment%3Flc%3D_COMMENT_ID_&amp;t=_COMMENT_TEXT_&#39;, {height:440, width:620, scrollbars:true});" />
                <img alt="" title="Twitter" src="/pixel.gif" class="icon-watch-comment-share icon-watch-comment-share-twitter" action="yt.window.popup(&#39;http:\/\/twitter.com\/share?url=http%3A%2F%2Fwww.youtube.com%2Fcomment%3Flc%3D_COMMENT_ID_&amp;text=Check+out+this+comment+--+_COMMENT_TEXT_&amp;via=youtube&#39;, {height:650, width:1024, scrollbars:true});" />
                <img alt="" title="Myspace" src="/pixel.gif" class="icon-watch-comment-share icon-watch-comment-share-myspace" action="yt.window.popup(&#39;http:\/\/www.myspace.com\/Modules\/PostTo\/Pages\/?t=_COMMENT_TEXT_&amp;u=http%3A%2F%2Fwww.youtube.com%2Fcomment%3Flc%3D_COMMENT_ID_&amp;l=1&#39;, {height:650, width:1024, scrollbars:true});" />
              </div>
            </div>
          </div>

