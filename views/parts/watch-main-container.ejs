                    <a class="author" href="/user/<%= comment.authorId %>" title="<%= comment.authorName %>"><%= comment.authorName %></a>

                    <span class="time"><%= comment.timeAgo %></span>
<% if ((comment.likeCount - comment.dislikeCount) > 0) { %>
                    <span class="comments-rating-positive" title="<%= comment.likeCount %> up, <%= comment.dislikeCount %> down"> <%= comment.likeCount - comment.dislikeCount %> <img class="comments-rating-thumbs-up" src="/pixel.gif" /> </span>
                  </div>
<% } %>
                </div>
              </li>
<% }) %>
            </ul>
          </div>
          <div class="comments-section" onmouseover="if (yt.www && yt.www.watch && yt.www.watch.livecomments) yt.www.watch.livecomments.handleCommentMouseEvent(this, event);" onmouseout="if (yt.www && yt.www.watch && yt.www.watch.livecomments) yt.www.watch.livecomments.handleCommentMouseEvent(this, event);">
            <div id="comments-header-container">
              <div id="comments-header">
                <a class="comments-section-see-all" href="/all_comments?v=<%= video.videoId %>">see all</a>
                <h4>All Comments (<%= video.commentCount %>)</h4>
              </div>
              <div id="reactions-header">
