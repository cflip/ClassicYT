<% topComments.forEach(comment => { %>
              <li class="comment" data-tag="R" data-author-viewing="" data-id="C6R9Wm_pyV_-Ofe6wPvwzFAUW3oaBEvgPme7pHyaH6o" data-score="48" data-author="<%= comment.authorName %>">
