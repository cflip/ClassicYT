              <div id="reactions-input">
                <button type="button" class="yt-uix-button-masked reaction-menu-button yt-uix-button" onclick=";return false;" role="button" aria-pressed="false" aria-expanded="false" aria-haspopup="true" aria-activedescendant="">
                  <span class="yt-uix-button-content">Your reaction?</span><img class="yt-uix-button-arrow" src="/pixel.gif" alt="" />
                  <div class="yt-uix-button-menu" style="display: none">
                    <div id="reactions-input-menu">
                      <ul id="reactions-input-items-menu">
                        <li role="menuitem">
                          <span class="yt-uix-button-menu-item"> <a href="https://accounts.google.com/ServiceLogin?uilel=3&amp;service=youtube&amp;passive=true&amp;continue=http%3A%2F%2Fwww.youtube.com%2Fsignin%3Faction_handle_signin%3Dtrue%26nomobiletemp%3D1%26hl%3Den_US%26next%3Dhttp%253A%252F%252Fwww.youtube.com%252Fwatch%253Fv%253D<%= video.videoId %>%2526gl%253DUS%2526hl%253Den%2526amp%253Bhas_verified%253D1&amp;hl=en_US&amp;ltmpl=sso">Sign in</a> to react to this video </span>
                        </li>
                      </ul>
                    </div>
                  </div>
                </button>
              </div>
            </div>

            <div id="live-comments-setting-scroll" class="live-comments-setting hid"><span id="live-comments-count"></span><a href="/web/20110918102336id_/http://www.youtube.com/watch?v=<%= video.videoId %>&amp;gl=US&amp;hl=en&amp;amp;has_verified=1#" onclick="yt.UserPrefs.setFlag3(yt.UserPrefs.Flags.FLAG3_LIVE_COMMENTS_SCROLL, true); yt.UserPrefs.save(); yt.www.watch.livecomments.setScroll(true); return false;">Update automatically</a></div>
            <div id="live-comments-setting-no-scroll" class="live-comments-setting hid"><a href="/web/20110918102336id_/http://www.youtube.com/watch?v=<%= video.videoId %>&amp;gl=US&amp;hl=en&amp;amp;has_verified=1#" onclick="yt.UserPrefs.setFlag3(yt.UserPrefs.Flags.FLAG3_LIVE_COMMENTS_SCROLL, false); yt.UserPrefs.save(); yt.www.watch.livecomments.setScroll(false); return false;">Disable automatic updates</a></div>
            <ul class="comment-list" id="live_comments">
<% comments.forEach(comment => { %>
                <li class="comment" data-tag="R" data-author-viewing="" data-id="C6R9Wm_pyV_-Ofe6wPvwzFAUW3oaBEvgPme7pHyaH6o" data-score="48" data-author="<%= comment.authorName %>">
                  <div class="comment-container">
                    <div class="content">
                      <div class="comment-text" dir="ltr">
                        <p><%= comment.text %></p>
                      </div>
                    </div>
  
  
                    <div class="metadata">
                      <a class="author" href="/user/<%= comment.authorId %>" title="<%= comment.authorName %>"><%= comment.authorName %></a>
  
                      <span class="time"><%= comment.timeAgo %></span>
  <% if ((comment.likeCount - comment.dislikeCount) > 0) { %>
                      <span class="comments-rating-positive" title="<%= comment.likeCount %> up, <%= comment.dislikeCount %> down"> <%= comment.likeCount - comment.dislikeCount %> <img class="comments-rating-thumbs-up" src="/pixel.gif" /> </span>
                    </div>
  <% } %>
                  </div>
                </li>
  <% }) %>
            </ul>
          </div>

          <div class="comments-section">
            <a class="comments-section-see-all" href="/all_comments?v=<%= video.videoId %>">View all Comments Â»</a>
            <div class="comments-pagination">
              <div class="yt-uix-pager">
                <button href="/all_comments?v=<%= video.videoId %>&amp;page=1" onclick="return false;;window.location.href=this.getAttribute(&#39;href&#39;);return false;" type="button" class="yt-uix-pager-selected yt-uix-button" data-page="1" role="button"><span class="yt-uix-button-content">1</span></button>&nbsp; <button href="/all_comments?v=<%= video.videoId %>&amp;page=2" onclick="yt.www.watch.livecomments.setScroll(false); return yt.www.comments.watch5.showPageFromElement(this);;window.location.href=this.getAttribute(&#39;href&#39;);return false;" type="button" class="yt-uix-button" data-page="2" role="button"><span class="yt-uix-button-content">2</span></button>&nbsp; <button href="/all_comments?v=<%= video.videoId %>&amp;page=3" onclick="yt.www.watch.livecomments.setScroll(false); return yt.www.comments.watch5.showPageFromElement(this);;window.location.href=this.getAttribute(&#39;href&#39;);return false;" type="button" class="yt-uix-button" data-page="3" role="button"><span class="yt-uix-button-content">3</span></button>&nbsp; <button href="/all_comments?v=<%= video.videoId %>&amp;page=4" onclick="yt.www.watch.livecomments.setScroll(false); return yt.www.comments.watch5.showPageFromElement(this);;window.location.href=this.getAttribute(&#39;href&#39;);return false;" type="button" class="yt-uix-button" data-page="4" role="button"><span class="yt-uix-button-content">4</span></button>&nbsp; <button href="/all_comments?v=<%= video.videoId %>&amp;page=5" onclick="yt.www.watch.livecomments.setScroll(false); return yt.www.comments.watch5.showPageFromElement(this);;window.location.href=this.getAttribute(&#39;href&#39;);return false;" type="button" class="yt-uix-button" data-page="5" role="button"><span class="yt-uix-button-content">5</span></button>&nbsp; <button href="/all_comments?v=<%= video.videoId %>&amp;page=6" onclick="yt.www.watch.livecomments.setScroll(false); return yt.www.comments.watch5.showPageFromElement(this);;window.location.href=this.getAttribute(&#39;href&#39;);return false;" type="button" class="yt-uix-button" data-page="6" role="button"><span class="yt-uix-button-content">6</span></button>&nbsp; <button href="/all_comments?v=<%= video.videoId %>&amp;page=7" onclick="yt.www.watch.livecomments.setScroll(false); return yt.www.comments.watch5.showPageFromElement(this);;window.location.href=this.getAttribute(&#39;href&#39;);return false;" type="button" class="yt-uix-button" data-page="7" role="button"><span class="yt-uix-button-content">7</span></button>&nbsp;

